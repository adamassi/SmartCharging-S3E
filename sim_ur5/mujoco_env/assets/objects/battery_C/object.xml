<mujoco model="battery_C">
    <asset>
        <!-- Add the date texture -->
        <texture name="battery_texture" type="2d" file="dated_image.png" />
        <material name="battery_material" texture="battery_texture" texrepeat="5 5" texuniform="true" />
    </asset>

    <worldbody>
        <body name="battery_C" pos="0 0 0">
            <!-- Main green body with texture -->
            <geom name="battery_body" type="cylinder" pos="0 0 0" size="0.0131 0.02175"
                  material="battery_material"
                  mass="0.07" friction="1 0.005 0.0001" condim="4"/>

            <!-- Bottom grey cap -->
            <geom name="battery_bottom_cap" type="cylinder" pos="0 0 -0.02425" size="0.0131 0.0025"
                  rgba="0.5 0.5 0.5 1" mass="0.01" friction="1 0.005 0.0001" condim="4"/>

            <!-- Top grey cap -->
            <geom name="battery_top_cap" type="cylinder" pos="0 0 0.02425" size="0.0131 0.0025"
                  rgba="0.5 0.5 0.5 1" mass="0.01" friction="1 0.005 0.0001" condim="4"/>

            <!-- Positive bump on top -->
            <geom name="battery_positive_bump" type="cylinder" pos="0 0 0.02575" size="0.004 0.0015"
                  rgba="0.5 0.5 0.5 1" mass="0.001" friction="1 0.005 0.0001" condim="4"/>

            <!-- <joint name="battery_C_joint" type="free"/> -->
        </body>
    </worldbody>
</mujoco>
