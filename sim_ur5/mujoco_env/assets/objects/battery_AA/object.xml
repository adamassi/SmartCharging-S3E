<mujoco model="battery_AA">
    <asset>
        <!-- Texture definition -->
        <texture name="battery_texture" type="2d" file="dated_image.png" />
        <material name="battery_material" texture="battery_texture" texrepeat="1 3" texuniform="true" />
    </asset>

    <worldbody>
        <body name="battery_AA" pos="0 0 0">
            <!-- Main green body with texture -->
            <geom name="battery_body" type="cylinder" pos="0 0 0" size="0.00725 0.0225"
                  material="battery_material"
                  mass="0.023" friction="1 0.005 0.0001" condim="4"/>

            <!-- Bottom grey cap -->
            <geom name="battery_bottom_cap" type="cylinder" pos="0 0 -0.02375" size="0.00725 0.00125"
                  rgba="0.5 0.5 0.5 1" mass="0.003" friction="1 0.005 0.0001" condim="4"/>

            <!-- Top grey cap -->
            <geom name="battery_top_cap" type="cylinder" pos="0 0 0.02375" size="0.00725 0.00125"
                  rgba="0.5 0.5 0.5 1" mass="0.003" friction="1 0.005 0.0001" condim="4"/>

            <!-- Positive bump on top -->
            <geom name="battery_positive_bump" type="cylinder" pos="0 0 0.02525" size="0.0025 0.00025"
                  rgba="0.5 0.5 0.5 1" mass="0.001" friction="1 0.005 0.0001" condim="4"/>

            <!-- <joint name="battery_AA_joint" type="free"/> -->
        </body>
    </worldbody>
</mujoco>
